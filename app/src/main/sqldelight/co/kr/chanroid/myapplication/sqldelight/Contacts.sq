CREATE TABLE CONTACTS (
    ADDRESS TEXT NOT NULL,
    EMAIL TEXT,
    GENDER TEXT NOT NULL,
    ID TEXT UNIQUE PRIMARY KEY,
    NAME TEXT NOT NULL
);

selectAll:
SELECT * FROM CONTACTS;

insertContact:
INSERT INTO CONTACTS (ADDRESS, EMAIL, GENDER, ID, NAME)
VALUES (?, ?, ?, ?, ?);

deleteContact:
DELETE FROM CONTACTS WHERE ID = ?;